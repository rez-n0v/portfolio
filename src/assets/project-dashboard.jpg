<svg width="800" height="450" viewBox="0 0 800 450" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="dashboardGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1d4ed8;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="450" fill="#f8fafc"/>
  
  <!-- Header -->
  <rect x="0" y="0" width="800" height="60" fill="url(#dashboardGradient)"/>
  <text x="20" y="35" fill="white" font-family="Arial, sans-serif" font-size="18" font-weight="bold">Analytics Dashboard</text>
  
  <!-- Sidebar -->
  <rect x="0" y="60" width="200" height="390" fill="#e2e8f0"/>
  <rect x="20" y="80" width="160" height="20" fill="#cbd5e1" rx="4"/>
  <rect x="20" y="110" width="160" height="20" fill="#cbd5e1" rx="4"/>
  <rect x="20" y="140" width="160" height="20" fill="#cbd5e1" rx="4"/>
  
  <!-- Main Content Area -->
  <rect x="220" y="80" width="560" height="200" fill="white" stroke="#e2e8f0" stroke-width="2" rx="8"/>
  
  <!-- Chart Area -->
  <rect x="240" y="100" width="520" height="160" fill="#f1f5f9"/>
  
  <!-- Chart Bars -->
  <rect x="260" y="200" width="40" height="40" fill="#3b82f6"/>
  <rect x="320" y="180" width="40" height="60" fill="#10b981"/>
  <rect x="380" y="160" width="40" height="80" fill="#f59e0b"/>
  <rect x="440" y="190" width="40" height="50" fill="#ef4444"/>
  <rect x="500" y="170" width="40" height="70" fill="#8b5cf6"/>
  <rect x="560" y="185" width="40" height="55" fill="#06b6d4"/>
  
  <!-- Chart Labels -->
  <text x="280" y="250" fill="#64748b" font-family="Arial, sans-serif" font-size="12">Jan</text>
  <text x="340" y="250" fill="#64748b" font-family="Arial, sans-serif" font-size="12">Feb</text>
  <text x="400" y="250" fill="#64748b" font-family="Arial, sans-serif" font-size="12">Mar</text>
  <text x="460" y="250" fill="#64748b" font-family="Arial, sans-serif" font-size="12">Apr</text>
  <text x="520" y="250" fill="#64748b" font-family="Arial, sans-serif" font-size="12">May</text>
  <text x="580" y="250" fill="#64748b" font-family="Arial, sans-serif" font-size="12">Jun</text>
  
  <!-- Stats Cards -->
  <rect x="220" y="300" width="180" height="100" fill="white" stroke="#e2e8f0" stroke-width="2" rx="8"/>
  <text x="240" y="325" fill="#1e293b" font-family="Arial, sans-serif" font-size="14" font-weight="bold">Total Users</text>
  <text x="240" y="350" fill="#3b82f6" font-family="Arial, sans-serif" font-size="24" font-weight="bold">12,543</text>
  <text x="240" y="370" fill="#64748b" font-family="Arial, sans-serif" font-size="12">+12% from last month</text>
  
  <rect x="420" y="300" width="180" height="100" fill="white" stroke="#e2e8f0" stroke-width="2" rx="8"/>
  <text x="440" y="325" fill="#1e293b" font-family="Arial, sans-serif" font-size="14" font-weight="bold">Revenue</text>
  <text x="440" y="350" fill="#10b981" font-family="Arial, sans-serif" font-size="24" font-weight="bold">$45,231</text>
  <text x="440" y="370" fill="#64748b" font-family="Arial, sans-serif" font-size="12">+8% from last month</text>
  
  <rect x="620" y="300" width="160" height="100" fill="white" stroke="#e2e8f0" stroke-width="2" rx="8"/>
  <text x="640" y="325" fill="#1e293b" font-family="Arial, sans-serif" font-size="14" font-weight="bold">Conversion</text>
  <text x="640" y="350" fill="#f59e0b" font-family="Arial, sans-serif" font-size="24" font-weight="bold">3.2%</text>
  <text x="640" y="370" fill="#64748b" font-family="Arial, sans-serif" font-size="12">+0.5% from last month</text>
</svg>
